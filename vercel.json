{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "api/index.js"
    },
    {
      "src": "/signup",
      "dest": "api/index.js"
    },
    {
      "src": "/login",
      "dest": "api/index.js"
    },
    {
      "src": "/api/users/me",
      "dest": "api/index.js"
    },
    {
      "src": "/api/users",
      "dest": "api/index.js"
    },
    {
      "src": "/api/trips/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/api/expenses/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/api/items/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/api/send-settlement-email",
      "dest": "api/index.js"
    },
    {
      "src": "/api/test-settlements",
      "dest": "api/index.js"
    },
    {
      "src": "/api/test-email",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}